<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_deeplinks">

    <fragment
        android:id="@+id/navigation_deeplinks"
        android:name="com.example.advandroidlab1.ui.deeplinks.DeeplinkFragment"
        android:label="@string/title_deeplinks"
        tools:layout="@layout/fragment_deeplinks" />

    <fragment
        android:id="@+id/navigation_services"
        android:name="com.example.advandroidlab1.ui.service.ServiceFragment"
        android:label="@string/title_service"
        tools:layout="@layout/fragment_service" />

    <fragment
        android:id="@+id/navigation_broadcast"
        android:name="com.example.advandroidlab1.ui.broadcast.BroadcastFragment"
        android:label="@string/title_broadcast"
        tools:layout="@layout/fragment_broadcast" />
    <fragment
        android:id="@+id/navigation_content_provider"
        android:name="com.example.advandroidlab1.ui.content_provider.ContentProviderFragment"
        android:label="fragment_content_provider"
        tools:layout="@layout/fragment_content_provider" />
</navigation>